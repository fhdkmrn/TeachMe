<p id="notice"><%= notice %></p>

<h1>Listing Courses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>

        <td><%=course.department %><%=course.course_no%> <%= course.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

