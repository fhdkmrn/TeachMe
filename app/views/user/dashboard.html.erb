<h1>Name: <strong><%= @user.name %></strong></h1>
<p>Find me in app/views/user/show.html.erb</p>

<%= form_tag search_path, method: :get do %>
  <p>
    <%= text_field_tag :query, params[:query] %>
    <%= submit_tag "Search", name: nil %>
  </p>
<% end %>

<div id="users">
  <h1>Users</h1>
  <%= @searchResults %>
<% @searchResults.each do |user| %>
  <h2>
    <%= user["name"] %>
  </h2>
<% end %>
</div>
